  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "tsc && node dist/app.js",
    "migrate:make": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:make --knexfile=src/db/knexfile.ts",
    "migrate:up": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:up --knexfile=src/db/knexfile.ts",
    "migrate:down": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:down --knexfile=src/db/knexfile.ts",
    "seed": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile=src/db/knexfile.ts",
    "test": "jest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.1",
    "bullmq": "^5.7.7",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.12.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.6",
    "@types/xlsx": "^0.0.36",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
